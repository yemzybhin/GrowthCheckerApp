                </androidx.cardview.widget.CardView>

            </GridLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_margin="30dp"
                android:background="@color/MenuItem"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/btn_saveimage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Back"
                    android:textColor="@color/primarycolor2"
                    app:cornerRadius="20dp"></Button>
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="35dp"
                    app:cardBackgroundColor="#00F5F6FC"
                    android:layout_margin="10dp"
                    android:layout_gravity="center"
                    app:cardElevation="0dp">
                    <ImageView
                        android:id="@+id/image17"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:src="@drawable/a56"
                        android:background="@drawable/imagecut"></ImageView>
                </androidx.cardview.widget.CardView>
                <TextView
                    android:id="@+id/tv_imagenum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="1"
                    android:textStyle="italic|bold"
                    android:textColor="@color/MenuItem"
                    android:textSize="16dp"></TextView>
            </LinearLayout>





        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cd_myinfopopupcancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/primary4"
        app:cardCornerRadius="30dp">

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="20dp"
            android:src="@drawable/cancel2"></ImageView>
    </androidx.cardview.widget.CardView>

    <View
        android:layout_width="20dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cd_myinfopopupcancel"></View>


</androidx.constraintlayout.widget.ConstraintLayout>