<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".Fragments.Pages.subpages.Analyticsfragment1">

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></View>


        <androidx.cardview.widget.CardView
            android:id="@+id/analytics1careed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="10dp"
            app:cardCornerRadius="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view4">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/homebg"
                android:layout_gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white"
                        android:layout_margin="15dp"
                        app:cardCornerRadius="25dp">
                        <ImageView
                            android:id="@+id/iv_analyticscurrentimage"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:src="@drawable/forteendays">
                        </ImageView>

                    </androidx.cardview.widget.CardView>
                    <TextView
                        android:id="@+id/tv_analyticscurrentchallenge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="14 Days Challenge"
                        android:layout_gravity="center"
                        android:layout_marginLeft="-10dp"
                        android:textSize="12sp"
                        android:fontFamily="@font/productsansbold"
                        android:textColor="@color/white"></TextView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Positive Adders  "
                        android:layout_marginLeft="20dp"
                        android:textColor="#F79748"
                        android:textSize="12sp"
                        android:fontFamily="@font/productsansbold">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <androidx.cardview.widget.CardView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                app:cardCornerRadius="20dp"
                                app:cardBackgroundColor="@color/white"
                                android:layout_marginTop="2dp">
                            </androidx.cardview.widget.CardView>
                            <TextView
                                android:id="@+id/tv_analytics1adder1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/productsansregular"
                                android:text="Read all the chapters"
                                android:textSize="12sp"
                                android:textColor="@color/white"></TextView>


                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <androidx.cardview.widget.CardView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                app:cardCornerRadius="20dp"
                                app:cardBackgroundColor="@color/white"
                                android:layout_marginTop="2dp">
                            </androidx.cardview.widget.CardView>
                            <TextView
                                android:id="@+id/tv_analytics1adder2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/productsansregular"
                                android:text="Read all the chapters"
                                android:textSize="12sp"
                                android:textColor="@color/white"></TextView>


                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <androidx.cardview.widget.CardView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                app:cardCornerRadius="20dp"
                                app:cardBackgroundColor="@color/white"
                                android:layout_marginTop="2dp">
                            </androidx.cardview.widget.CardView>
                            <TextView
                                android:id="@+id/tv_analytics1adder3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:fontFamily="@font/productsansregular"
                                android:layout_marginBottom="8dp"
                                android:text="Read all the chapters"
                                android:textSize="12sp"
                                android:textColor="@color/white"></TextView>


                        </LinearLayout>



                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Recently Added Points:   "
                        android:layout_marginLeft="20dp"
                        android:textColor="#F79748"
                        android:textSize="12sp"
                        android:fontFamily="@font/productsansbold">
                    </TextView>
                    <TextView
                        android:id="@+id/tv_analytics1recentlyadded"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        android:layout_marginBottom="15dp"
                        android:textSize="12sp"
                        android:fontFamily="@font/productsansregular"
                        android:text="..."
                        android:textColor="@color/white"></TextView>
                </LinearLayout>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="18dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.github.mikephil.charting.charts.LineChart
                                android:id="@+id/lc_graphforanalytics1"
                                android:layout_width="match_parent"
                                android:layout_marginTop="20dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:layout_height="200dp"
                                android:padding="9dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/tv_nochartdata11"
                                android:layout_width="200dp"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:gravity="center"
                                android:text="Not Enough Data to display graph. After the first assessment, graph would be displayed"
                                android:textColor="@color/primary5"
                                android:textSize="12dp"
                                android:textStyle="italic"></TextView>

                        </RelativeLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="20dp"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/view3"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#A1A1A1"></View>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:text="Days"
                                    android:textColor="@color/textsub"
                                    android:textSize="10sp"
                                    android:textStyle="bold"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:text="Points"
                                    android:textColor="@color/textsub"
                                    android:textSize="10sp"
                                    android:textStyle="bold"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:text="Cumulative"
                                    android:textColor="@color/textsub"
                                    android:textSize="10sp"
                                    android:textStyle="bold"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>
                            </LinearLayout>

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1.5dp"
                                android:background="#A1A1A1"></View>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:id="@+id/tv_analyticstabcolumn1"
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:textColor="@color/textsub"
                                    android:text="..."
                                    android:textSize="10sp"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:id="@+id/tv_analyticstabcolumn2"
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:textColor="@color/textsub"
                                    android:text="..."
                                    android:textSize="10sp"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>

                                <TextView
                                    android:id="@+id/tv_analyticstabcolumn3"
                                    android:layout_width="60dp"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:gravity="center"
                                    android:textColor="@color/textsub"
                                    android:text="..."
                                    android:textSize="10sp"></TextView>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#A1A1A1"></View>
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#A1A1A1"></View>

                        </LinearLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

    <View
        android:id="@+id/viewjx"
        android:layout_width="match_parent"
        android:layout_height="7dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analytics1careed"></View>




</androidx.constraintlayout.widget.ConstraintLayout>